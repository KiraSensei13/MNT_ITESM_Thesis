pages = {268--275},
pmid = {24257507},
title = {{Light-assisted direct-write of 3D functional biomaterials}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24257507 http://xlink.rsc.org/?DOI=C3LC50634G},
volume = {14},
year = {2014}
}
