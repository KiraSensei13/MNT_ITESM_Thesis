@article{Zheng2014,
abstract = {AC pulse-modulated electrohydrodynamic direct-writing (EDW) was utilized to direct-write orderly micro/nanofibrous structure on the flexible insulating polyethylene terephthalate (PET) substrate. During the EDW process, AC electrical field induced charges to reciprocate along the jet and decreased the charge repulsive force that applied on charged jet. Thanks to the smaller charge repulsive force, stable straight jet can be built up to direct-write orderly micro/nanofibrous structures on the insulating substrate. The minimum motion velocity required to direct-write straight line fibrous structure on insulating PET substrate was 700 mm/s. Moreover, the influences of AC voltage amplitude, frequency, and duty cycle ratio on the line width of fibrous structures were investigated. This work proposes a novel solution to overcome the inherent charge repulsion emerging on the insulating substrate, and promotes the application of EDW technology on the flexible electronics.},
author = {Zheng, Jiang-Yi and Liu, Hai-Yan and Wang, Xiang and Zhao, Yang and Huang, Wei-Wei and Zheng, Gao-Feng and Sun, Dao-Heng},
doi = {10.1155/2014/708186},
file = {:C$\backslash$:/Users/oskat/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zheng et al. - 2014 - Electrohydrodynamic Direct-Write Orderly MicroNanofibrous Structure on Flexible Insulating Substrate.pdf:pdf},
issn = {1687-4110},
journal = {Journal of Nanomaterials},
keywords = {NFES},
mendeley-groups = {0.1{\_}NearFieldElectrospinning},
mendeley-tags = {NFES},
month = {may},
pages = {1--7},
publisher = {Hindawi},
title = {{Electrohydrodynamic Direct-Write Orderly Micro/Nanofibrous Structure on Flexible Insulating Substrate}},
