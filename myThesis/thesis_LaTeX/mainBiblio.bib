publisher = {Elsevier Ltd},
title = {{Electrospun polyimide nanofibers and their applications}},
url = {http://dx.doi.org/10.1016/j.progpolymsci.2016.06.006 https://linkinghub.elsevier.com/retrieve/pii/S0079670016300387},
