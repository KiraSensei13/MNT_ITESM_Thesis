issn = {1424-8220},
journal = {Sensors},
keywords = {absorption spectrophotometry,btex,deep ultraviolet,hollow core waveguides,leds,no 2,ozone,photodiodes,so 2,spectroscopy,ultraviolet light sources,voc},
month = {nov},
number = {23},
pages = {5210},
title = {{Gas Detection Using Portable Deep-UV Absorption Spectrophotometry: A Review}},
url = {https://www.mdpi.com/1424-8220/19/23/5210},
volume = {19},
year = {2019}
}
