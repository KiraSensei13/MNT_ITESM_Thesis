keywords = {Interface,Polymer nanocomposite,Rheology,Silica aerogel},
month = {dec},
number = {6},
pages = {1755--1765},
publisher = {Springer US},
title = {{Water-glass based silica aerogel: unique nanostructured filler for epoxy nanocomposites}},
url = {https://doi.org/10.1007/s10934-019-00757-3 http://link.springer.com/10.1007/s10934-019-00757-3},
