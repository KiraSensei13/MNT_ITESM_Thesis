file = {:C$\backslash$:/Users/oskat/OneDrive - Instituto Tecnologico y de Estudios Superiores de Monterrey/Documents/MNT{\_}ITESM{\_}courses/0.References/2009{\_}Book{\_}FoodChemistry.pdf:pdf},
isbn = {978-3-540-69933-0},
