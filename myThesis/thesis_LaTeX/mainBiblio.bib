issn = {13640321},
journal = {Renewable and Sustainable Energy Reviews},
keywords = {Effect of temperature,Heat transfer,Nanofluids,Nanoparticle influence,Surface tension,Wettability},
month = {oct},
number = {November 2017},
pages = {931--944},
title = {{Current trends in surface tension and wetting behavior of nanofluids}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S1364032118305136},
volume = {94},
year = {2018}
}
