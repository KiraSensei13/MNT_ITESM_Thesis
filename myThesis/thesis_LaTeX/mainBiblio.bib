publisher = {Elsevier B.V.},
title = {{Electrical conductivity of compacts of graphene, multi-wall carbon nanotubes, carbon black, and graphite powder}},
url = {http://dx.doi.org/10.1016/j.powtec.2012.01.024 https://linkinghub.elsevier.com/retrieve/pii/S0032591012000277},
