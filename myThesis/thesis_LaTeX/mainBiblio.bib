issn = {09284931},
journal = {Materials Science and Engineering: C},
keywords = {Cell infiltration,Layer-structured scaffold,NFES,Near-field electrospinning,Osteogenic differentiation,Tissue engineering},
mendeley-groups = {0.1{\_}NearFieldElectrospinning},
mendeley-tags = {NFES},
month = {may},
