pages = {18--27},
publisher = {Elsevier},
title = {{A novel layer-structured scaffold with large pore sizes suitable for 3D cell culture prepared by near-field electrospinning}},
url = {https://doi.org/10.1016/j.msec.2017.12.016 https://linkinghub.elsevier.com/retrieve/pii/S0928493117325390},
