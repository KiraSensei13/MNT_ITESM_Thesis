file = {:C$\backslash$:/Users/oskat/OneDrive - Instituto Tecnologico y de Estudios Superiores de Monterrey/Documents/MNT{\_}ITESM{\_}Thesis/relatedArticles/ABITR.pdf:pdf},
pages = {1--20},
title = {{Malvern Instruments White Paper - A Basic Introduction to Rheology Shear Flow}},
url = {https://www.malvernpanalytical.com/en/learn/knowledge-center/whitepapers/WP160620BasicIntroRheology.html},
