@article{Pan2020,
abstract = {In this study, a new type of fiber-based mechanomyography (MMG) sensor was developed and used as a motion on/off trigger sensor for a lower limb rehabilitation exoskeleton (LLRE). Piezoelectric material, polyvinylidene difluoride (PVDF), was applied to fabricate a MMG sensor, using the near-field electrospinning technology to electrically spin PVDF fibers to ∼5-10 $\mu$m in diameter. PVDF fibers attached on an interdigitated (IDT) electrode with different pole pairs and interspaces were packaged into a MMG sensor. This MMG sensor was stuck on the thigh muscles to detect and acquire human motion intention signal, then trigger the controller and actuate the LLRE. The LLRE multi-axis control system included a master and four slave controllers. The master controller released the command signal to control the slave controllers via controller area network (CAN). A multi-axis control system was developed to actuate the joints of hips and knees of both legs by the LLRE. In the study, commercial electromyography (EMG) sensors were also to compare with the as-made MMG sensors. Results showed that the maximum signal amplitude of the commercail EMG sensor was about ∼0.2 V, whereas the MMG sensor was about ∼2.8 V. The signal-to-noise ratio (SNR) of EMG was about ∼4, while that of MMG was about ∼25. From physiological signals captured from the MMG sensor for the human walking with the LLRE, this new sensor improved the sensitivity in driving the LLRE.},
author = {Pan, Cheng-Tang and Chang, Chun-Chieh and Yang, Yu-Sheng and Yen, Chung-Kun and Kao, Yu-Hsuan and Shiue, Yow-Ling},
doi = {10.1016/j.sna.2019.111708},
file = {:C$\backslash$:/Users/oskat/OneDrive - Instituto Tecnologico y de Estudios Superiores de Monterrey/Documents/MNT{\_}ITESM{\_}Thesis/relatedArticles/DOMSUPPEFLLRE.pdf:pdf},
issn = {09244247},
journal = {Sensors and Actuators A: Physical},
keywords = {Electromyography (EMG),Lower limb rehabilitation exoskeleton (LLRE),Mechanomyography (MMG),Near-field electrospinning technology,Piezoelectric,Polyvinylidene difluoride (PVDF)},
month = {jan},
pages = {111708},
publisher = {Elsevier B.V.},
title = {{Development of MMG sensors using PVDF piezoelectric electrospinning for lower limb rehabilitation exoskeleton}},
url = {https://doi.org/10.1016/j.sna.2019.111708 https://linkinghub.elsevier.com/retrieve/pii/S0924424719304157},
volume = {301},
year = {2020}
}
