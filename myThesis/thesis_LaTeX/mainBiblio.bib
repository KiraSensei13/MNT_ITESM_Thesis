doi = {10.1021/bm400068k},
file = {:C$\backslash$:/Users/oskat/OneDrive - Instituto Tecnologico y de Estudios Superiores de Monterrey/Documents/MNT{\_}ITESM{\_}courses/0.References/EBAARTFPSOSFBM.pdf:pdf},
issn = {1525-7797},
journal = {Biomacromolecules},
