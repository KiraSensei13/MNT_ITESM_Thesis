journal = {Journal of Environmental Sciences},
keywords = {mitigation,temperature rise,urban heat island (UHI),urbanization},
month = {jan},
number = {1},
pages = {120--128},
pmid = {18572534},
publisher = {Elsevier},
title = {{A review on the generation, determination and mitigation of Urban Heat Island}},
url = {https://www.sciencedirect.com/science/article/pii/S1001074208600194 https://linkinghub.elsevier.com/retrieve/pii/S1001074208600194},
