isbn = {9780735410626},
issn = {0094243X},
journal = {AIP Conference Proceedings},
keywords = {electrospinning,nanowires,polyaniline},
number = {1},
pages = {253--255},
title = {{Polyaniline nanofibers: Towards pure electrospun PANI}},
volume = {1459},
year = {2012}
}
@article{Sarkar2007,
