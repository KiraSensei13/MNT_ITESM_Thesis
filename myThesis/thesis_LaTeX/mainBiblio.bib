issn = {0003-2700},
journal = {Analytical Chemistry},
month = {jan},
number = {1},
pages = {105--111},
title = {{Optical Waveguide Sensor Based on a Porous Anodic Alumina/Aluminum Multilayer Film}},
url = {https://pubs.acs.org/doi/10.1021/ac8015642},
volume = {81},
year = {2009}
