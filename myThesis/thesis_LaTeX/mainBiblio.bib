issn = {09253467},
journal = {Optical Materials},
keywords = {AgNWs,Electrospinning,Near-field,PVP,Transparent conductive},
month = {jan},
pages = {118--124},
title = {{Synthesis and fabrication of silver nanowires embedded in PVP fibers by near-field electrospinning process}},
