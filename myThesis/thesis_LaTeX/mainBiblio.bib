@article{Pena-Pereira2011,
abstract = {UV-Vis spectrometry has become a widespread analytical technique due to its inherent features (e.g., simplicity, ease of operation, convenience and economy). The necessity to decrease the consumption of valuable and scarce samples, together with the application of novel sample-retention technologies, the improvement of materials and the development of microfluidics, has enabled development of a new generation of miniaturized UV-Vis spectrometric systems and accessories. This article surveys the state of the art on microsample-quantitation systems in UV-Vis spectrometry. {\textcopyright} 2011 Elsevier Ltd.},
