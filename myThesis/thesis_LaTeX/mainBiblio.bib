issn = {1070-4965},
journal = {The Journal of Environment {\&} Development},
keywords = {Hyderabad,India,declining yields,fodder grass,groundwater,irrigation,local innovation,rice,small farmers,wastewater,water management,water pollution},
month = {dec},
