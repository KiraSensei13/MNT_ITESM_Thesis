doi = {10.1016/j.biomaterials.2016.10.040},
file = {:C$\backslash$:/Users/oskat/OneDrive - Instituto Tecnologico y de Estudios Superiores de Monterrey/Documents/MNT{\_}ITESM{\_}Thesis/relatedArticles/EVMSOFFFBA.pdf:pdf},
