issn = {0956053X},
journal = {Waste Management},
keywords = {Kinetics,Py-GC/MS,Pyrolysis,Reaction mechanism,Waste tires},
month = {feb},
pages = {21--29},
publisher = {Elsevier Ltd},
title = {{Waste tires pyrolysis kinetics and reaction mechanisms explained by TGA and Py-GC/MS under kinetically-controlled regime}},
