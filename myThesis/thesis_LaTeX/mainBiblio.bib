issn = {09284931},
journal = {Materials Science and Engineering C},
keywords = {Controlled release,Keywords,Nanofibers,Poly(lactic acid),Progesterone},
number = {2},
pages = {844--849},
publisher = {Elsevier B.V.},
title = {{Development of poly(lactic acid) nanostructured membranes for the controlled delivery of progesterone to livestock animals}},
url = {http://dx.doi.org/10.1016/j.msec.2012.10.032},
