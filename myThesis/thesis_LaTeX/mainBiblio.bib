journal = {Biotechnology Advances},
keywords = {Collector,Electric jet,Electrospinning,Nanofibers,Non-woven mats,Polymer solution,Solvents},
number = {3},
pages = {325--347},
title = {{Electrospinning: A fascinating fiber fabrication technique}},
