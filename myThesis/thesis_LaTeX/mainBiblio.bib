issn = {0022-0299},
journal = {Journal of Dairy Research},
keywords = {Citrate,fraud,milk adulteration,sucrose,water},
month = {aug},
number = {3},
pages = {337--340},
title = {{X-ray diffraction has limited applicability in investigation of milk tampering}},
url = {https://www.cambridge.org/core/product/identifier/S0022029919000566/type/journal{\_}article},
volume = {86},
year = {2019}
