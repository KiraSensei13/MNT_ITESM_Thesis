issn = {09254005},
journal = {Sensors and Actuators B: Chemical},
keywords = {Analytical separations,Deep UV spectral range,Light emitting diodes (LEDs),Optical detection,Portable analytical instrumentation},
month = {feb},
pages = {1238--1243},
title = {{High power deep UV-LEDs for analytical optical instrumentation}},
