journal = {Acta Geochimica},
keywords = {Adsorption,Copper ion,Nanoporous silica},
month = {aug},
number = {4},
pages = {517--529},
publisher = {Science Press},
title = {{Adsorption characteristics of copper ion on nanoporous silica}},
url = {https://doi.org/10.1007/s11631-019-00358-6 http://link.springer.com/10.1007/s11631-019-00358-6},
volume = {38},
year = {2019}
}
