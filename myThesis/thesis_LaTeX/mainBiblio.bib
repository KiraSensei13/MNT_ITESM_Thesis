journal = {Sensors},
keywords = {Carbon nanotubes,Nanocomposites,Piezoresistivity,Printed electronics,Strain sensors},
month = {jun},
number = {6},
pages = {10042--10071},
title = {{Potential of Flexible Carbon Nanotube Films for High Performance Strain and Pressure Sensors}},
