journal = {Nature Medicine},
month = {may},
pmid = {32382154},
title = {{Modeling shield immunity to reduce COVID-19 epidemic spread}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/32382154 http://www.nature.com/articles/s41591-020-0895-3 https://www.nature.com/articles/s41591-020-0895-3},
year = {2020}
}
