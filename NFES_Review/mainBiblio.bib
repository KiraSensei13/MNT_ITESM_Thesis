editor = {Chin, Wei-Chun},
file = {:C$\backslash$:/Users/oskat/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Xue et al. - 2014 - Rapid Patterning of 1-D Collagenous Topography as an ECM Protein Fibril Platform for Image Cytometry.pdf:pdf},
issn = {1932-6203},
journal = {PLoS ONE},
month = {apr},
number = {4},
pages = {e93590},
publisher = {Public Library of Science},
title = {{Rapid Patterning of 1-D Collagenous Topography as an ECM Protein Fibril Platform for Image Cytometry}},
url = {https://dx.plos.org/10.1371/journal.pone.0093590},
volume = {9},
