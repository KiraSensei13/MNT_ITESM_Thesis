journal = {Nanoscale},
pages = {11637--11642},
title = {{Near-field electrospinning of conjugated polymer light-emitting nanofibers}},
url = {https://arxiv.org/ftp/arxiv/papers/1310/1310.5101.pdf},
volume = {5},
year = {2013}
}
