keywords = {PBLG,cylindrical near-field electrospinning,piezoelectric fibers},
number = {2},
pages = {63--73},
title = {{Poly($\gamma$-benzyl $\alpha$, l-glutamate) in Cylindrical Near-Field Electrospinning Fabrication and Analysis of Piezoelectric Fibers}},
